{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "api/*",
        "use": "@vercel/node"
      },
      {
        "src": "/(.*)",
        "use": "rewrites",
        "destination": "index.html"
      }
    ]
  }
  